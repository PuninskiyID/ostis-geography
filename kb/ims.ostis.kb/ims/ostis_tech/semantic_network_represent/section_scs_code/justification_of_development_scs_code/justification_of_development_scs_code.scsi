scs_nrel_representation_scs_editor
<- sc_node_norole_relation;
=> nrel_main_idtf:
	[отображение в sc.s-редакторе*]
	(* <- lang_ru;; *);
	[sc.s-editor representation*]
	(* <- lang_en;; *);
<- binary_relation;
<- oriented_relation;
=> nrel_first_domain:
	scs_divider;
=> nrel_second_domain:
	file;
=> nrel_definitional_domain:
	...
	(*
		<= nrel_combination:
		{
			scs_divider;
			file
		};;
	*);;

