scs_code
<- sc_node_not_relation;
=> nrel_idtf:
	[Язык SCs] (* <- lang_ru;; *);
	[sc.s-текст] (* <- lang_ru;; *);

=> nrel_inclusion:
	scs_code_core;

<- rrel_key_sc_element:
			...
			(*
				<- explanation;;
				<= nrel_sc_text_translation:	
				...
				(*
					-> rrel_example:
					"file://nl_files/explanation_for_scs_code.html"
					(*
					<- lang_ru;;
					=> nrel_format: format_html;;
					*);;
				*);;
			*);;


